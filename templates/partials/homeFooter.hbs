<script>
    $(".testimonial-slider").slick({
        // normal options...
        infinite: true,
        autoplay: false,
        autoplaySpeed: 2000,
        loop: true,
        dots: false,
        // the magic
    });

    $(".main-slider").slick({
        // normal options...
        infinite: true,
        autoplay: false,
        autoplaySpeed: 5000,
        pauseOnHover: false,
        loop: true,
        dots: true,
        cssEase: "ease",
    });
    $(".main-slider-text").slick({
        // normal options...
        infinite: true,
        autoplay: true,
        autoplaySpeed: 5000,
        pauseOnHover: false,
        loop: true,
        dots: false,
        fade: true,
        cssEase: "ease",
        arrows: false,
    });

    $(".media-slider").slick({
        // normal options...
        infinite: true,
        autoplay: true,
        autoplaySpeed: 500,
        loop: true,
        pauseOnHover: false,
        arrows: false,
        // the magic
        responsive: [
            {
                breakpoint: 1024,
                settings: {
                    slidesToShow: 3,
                    infinite: true,
                },
            },
            {
                breakpoint: 900,
                settings: {
                    slidesToShow: 2,
                },
            },
            {
                breakpoint: 560,
                settings: {
                    slidesToShow: 1,
                },
            },
            {
                breakpoint: 280,
                settings: "unslick", // destroys slick
            },
        ],
    });
    playVideo = () => {
        $("#youtubeVideoDiv").addClass("show-flex");
        $("#youtubeVideoDIv").removeClass("hide");
        document.getElementById("youtubeFrame").src =
            "https://www.youtube.com/embed/zYVxjIPVCZQ?rel=0&modestbranding=1&autohide=1&showinfo=0&autoplay=1";
    };
    closeVideo = () => {
        $("#youtubeVideoDiv").addClass("hide");
        $("#youtubeVideoDiv").removeClass("show-flex");
        document.getElementById("youtubeFrame").src = "";
    };

    $("body").keydown(function (e) {
        if (e.keyCode == 27) {
            // put your code here if any (that will run after Esc pressed).
            closeVideo();
        }
    });
    $(function () {
        //create instance
        $(".chart").easyPieChart({
            animate: 2000,
            barColor: "#980000",
            trackColor: "#ffffff",
            lineWidth: "15",
            size: "150",
        });
    });
